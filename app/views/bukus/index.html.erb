<p id="notice"><%= notice %></p>

<h1>Bukus</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bukus.each do |buku| %>
      <tr>
        <td><%= link_to 'Show', buku %></td>
        <td><%= link_to 'Edit', edit_buku_path(buku) %></td>
        <td><%= link_to 'Destroy', buku, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Buku', new_buku_path %>
